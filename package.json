    "react-gravatar": "^2.6.3",
    "react-intersection-observer": "^8.10.0",
    "react-lazy-load": "^3.0.13",
    "react-lazy-load-image-component": "^1.3.2",
    "react-loader-spinner": "^2.3.0",
    "react-portal": "^4.2.0",
    "react-router-dom": "^4.3.1",
